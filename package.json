{
  "name": "chatbotix",
  "version": "1.0.0",
  "scripts": {
    "dev": "vite",
    "dev:debug": "vite --debug",
    "build": "tsc && vite build",
    "build:prod": "cross-env NODE_ENV=production tsc && vite build",
    "build:staging": "cross-env NODE_ENV=staging tsc && vite build",
    "preview": "vite preview",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src --ext ts,tsx",
    "lint:fix": "eslint src --ext ts,tsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "typecheck": "tsc --noEmit",
    "analyze": "vite build --analyze",
    "clean": "rimraf dist",
    "prepare": "husky install",
    "deploy": "vercel --prod",
    "deploy:dev": "vercel --env development",
    "deploy:staging": "vercel --env staging",
    "deploy:prod": "vercel --prod --env production",
    "deploy:preview": "vercel --env preview",
    "deploy:branch": "vercel --env development --build-env BRANCH=$(git branch --show-current)",
    "preview:production": "vercel --prod --env production --preview-only",
    "deploy:notify": "vercel --prod --env production --with-notify",
    "deploy:analytics": "vercel --prod --env production --with-analytics"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/node": "^22.13.10",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "@vitejs/plugin-react": "^4.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.38.0",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "prettier": "^2.8.8",
    "rimraf": "^5.0.0",
    "typescript": "^5.8.2",
    "vite": "^4.3.0"
  }
}
