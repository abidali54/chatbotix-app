{
  "name": "chatbot-sales-platform",
  "version": "1.0.0",
  "description": "A comprehensive chatbot sales agent platform with multi-platform integration",
  "scripts": {
    "dev": "concurrently \"npm run dev:frontend\" \"npm run dev:backend\"",
    "dev:frontend": "cd frontend && npm run dev",
    "dev:backend": "cd backend && npm run dev",
    "install:all": "npm install && cd frontend && npm install && cd ../backend && npm install"
  },
  "keywords": [
    "chatbot",
    "sales",
    "whatsapp",
    "facebook",
    "instagram",
    "messenger"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^8.2.2",
    "nodemon": "^3.1.9"
  },
  "dependencies": {
    "mysql2": "^3.12.0"
  }
}
